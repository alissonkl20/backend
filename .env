# .env
# Configuração do PostgreSQL
DATABASE_URL=postgresql://postgres:123456@localhost:5432/food
DB_USERNAME=postgres
DB_PASSWORD=123456
DB_NAME=food
DB_HOST=localhost
DB_PORT=5432

# Configuração do SQLAlchemy (equivalente ao Hibernate em Python)
SQLALCHEMY_DATABASE_URI=postgresql://postgres:123456@localhost:5432/food
SQLALCHEMY_TRACK_MODIFICATIONS=False
SQLALCHEMY_ECHO=True  # Equivalente a show-sql
SQLALCHEMY_ENGINE_OPTIONS='{"echo": true}'  # Para formatar SQL

# Configurações de CORS (para Flask-CORS)
CORS_ALLOWED_ORIGINS=http://localhost:5173,http://127.0.0.1:5173
CORS_SUPPORTS_CREDENTIALS=True

# Configuração do Flask
FLASK_DEBUG=True
SECRET_KEY=less  # Adicione uma chave segura